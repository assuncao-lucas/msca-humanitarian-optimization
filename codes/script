time_limit=3600

num_vehicles_vec=(2 4)
service_time_deviation_vec=(0.10 0.25)
uncertainty_budget_vec=(1 5)

cont=0
for file in $dir0/*.txt
do
	files[$cont]="$file"
	((cont++))	
done

readarray -t sorted < <(printf '%s\0' "${files[@]}" | sort -z | xargs -0n1)

